{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "importsNotUsedAsValues": "error"
  },
  "include": ["**/*.ts", "**/*.js"]
}
