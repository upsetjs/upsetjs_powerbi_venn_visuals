        {
          "elements": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "for": { "in": "elements" },
          "dataReductionAlgorithm": {
            "window": {
              "count": 10000
            }
          }
        },
        "values": {
          "select": [{ "bind": { "to": "sets" } }]
        }
      }
    }
