    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "powerbi-visuals-tools": "^3.1.15",
    "powerbi-visuals-webpack-plugin": "2.2.5",
    "prettier": "^2.0.5",
    "release-it": "^13.6.7",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.9.7"
  }
}
