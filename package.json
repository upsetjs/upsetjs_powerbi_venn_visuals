{
  "name": "@upsetjs/powerbi_venn_visuals",
  "description": "UpSet.js is a re-implementation of UpSetR to create interactive set visualizations for more than three sets",
  "version": "1.9.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "homepage": "https://github.com/upsetjs/upsetjs_powerbi_venn_visuals",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs_powerbi_venn_visuals/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/upsetjs_powerbi_venn_visuals.git"
  },
  "scripts": {
    "pbiviz": "pbiviz",
    "start": "pbiviz start",
    "test": "echo no tests",
    "build:preview": "shx mv pbiviz.json pbiviz.bak.json && shx cp pbiviz_preview.json pbiviz.json && pbiviz package && shx mv dist/upSetJSVenn* dist/upsetjs_venn_preview.pbiviz && shx rm pbiviz.json && shx mv pbiviz.bak.json pbiviz.json",
    "build": "shx rm -rf dist && npm run build:preview && pbiviz package && shx cp dist/upSetJSVenn* dist/upsetjs_venn.pbiviz",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier:impl": "prettier \"*.{md,json,yml}\" .eslintrc.js .prettierrc.js \"{src,scripts,.github}/**\"",
    "prettier:write": "npm run prettier:impl -- --write",
    "prettier": "npm run prettier:impl -- --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "posttest": "npm run lint",
    "release": "release-it --disable-metrics",
    "package": "npm run build"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.16.5",
    "@upsetjs/bundle": "~1.10.1",
    "@upsetjs/venn.js": "^1.4.2",
    "core-js": "^3.20.1",
    "powerbi-visuals-api": "^4.0.0",
    "powerbi-visuals-utils-dataviewutils": "^2.4.1"
  },
  "devDependencies": {
    "@types/d3-selection": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "powerbi-visuals-tools": "^4.0.2",
    "powerbi-visuals-webpack-plugin": "3.0.0",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "shx": "^0.3.3",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.5.4"
  }
}
