  },
  "scripts": {
    "pbiviz": "pbiviz",
    "start": "pbiviz start",
    "test": "echo no tests",
    "build": "npx shx rm -R dist && pbiviz package && npx shx cp dist/upSetJSVenn* dist/upsetjs_venn.pbiviz",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier:impl": "prettier \"*.{md,json,yml}\" .eslintrc.js .prettierrc.js \"{src,scripts,.github}/**\"",
    "prettier:write": "npm run prettier:impl -- --write",
    "prettier": "npm run prettier:impl -- --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "posttest": "npm run lint",
    "release": "release-it --disable-metrics",
    "package": "npm run build"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.11.2",
    "@upsetjs/bundle": "~1.6.2",
    "@upsetjs/venn.js": "^1.3.0",
    "core-js": "^3.6.5",
    "powerbi-visuals-api": "^3.2.0",
    "powerbi-visuals-utils-dataviewutils": "^2.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
